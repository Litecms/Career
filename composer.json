{
    "name": "litecms/career",
    "description": "Career package for litecms.",
    "keywords": ["lavalite", "Career"],
    "license": "MIT",
    "type": "lavalite-package",
    "authors": [
        {
            "name": "Lavalite",
            "email": "info@lavalite.org"
        }
    ],
    "require": {
        "php": "^7.1.3"
    },
    "autoload": {
        "classmap": [
            "database/seeds"
        ],
        "psr-4": {
            "Litecms\\Career\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Litecms\\Career\\Providers\\CareerServiceProvider"
            ],
            "aliases": {
                "Career"       : "Litecms\\Career\\Facades"
            }
        }
    },
    "minimum-stability": "dev"
}
